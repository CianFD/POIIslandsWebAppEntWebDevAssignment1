<div class="uk-width-1-2@m  uk-card uk-card-default uk-padding-small uk-visible@m">
  <div class="uk-grid">
    <div class="uk-width-1-6@m">
      <img src="/images/titlecard.jpg" width="60" alt="islands-of-ireland-title-card" />
    </div>
    <div class="uk-width-expand@m uk-text-left">
      <div class="title">
        {{title}}
      </div>
      <div class="uk-text-muted uk-text-small">
        {{subTitle}}
      </div>
    </div>
    <div class="uk-card uk-card-small uk-text-center uk-padding-remove">
      <a target="_blank" href="https://github.com/CianFD/EntWebDevIslandsPOIWebApp">
        <div>
          <i class="fab fa-github fa-3x" style="color:rgb(95, 96, 173)" title="Git Repo" pos="bottom" uk-tooltip></i>
        </div>
        <div class="uk-width-expand@m uk-text-left">
          <div class="uk-text-muted uk-text-center" style="font-size:xx-small">Assignment1</div>
        </div>
      </a>
    </div>
  </div>
</div>
